@keyframes spinnerRotate { 
  0% { transform: rotate(0deg); }
  100% { transform: rotate(360deg); }
}

.spinner, input[type="submit"].loading::before, input[type="button"].loading::before, button.loading::before, .btn.loading::before, [class^="btn-"].loading::before { position: relative; display: inline-block; border-width: 2px; border-style: solid; border-color: rgba(255, 255, 255, 0.3) rgba(255, 255, 255, 0.3) rgba(255, 255, 255, 0.3) rgb(255, 255, 255); border-image: initial; transform: translateZ(0px); animation: spinnerRotate 0.8s linear 0s infinite normal none running; }

.spinner, .spinner::after, input[type="submit"].loading::before, input[type="button"].loading::before, button.loading::before, .btn.loading::before, [class^="btn-"].loading::before { width: 9px; height: 9px; border-radius: 50%; }

.spinner.gray::before { border-color: rgb(188, 212, 230) rgb(188, 212, 230) rgb(188, 212, 230) rgb(113, 165, 203); }

.spinner-gray { position: relative; display: inline-block; border-width: 2px; border-style: solid; border-color: rgb(188, 212, 230) rgb(188, 212, 230) rgb(188, 212, 230) rgb(113, 165, 203); border-image: initial; transform: translateZ(0px); animation: spinnerRotate 0.8s linear 0s infinite normal none running; }

.spinner-gray, .spinner-gray::after { width: 9px; height: 9px; border-radius: 50%; }

a.loading::before { content: ""; position: relative; display: inline-block; top: 1px; left: -4px; width: 8px; height: 8px; border-radius: 50%; border-width: 2px; border-style: solid; border-color: rgb(188, 212, 230) rgb(188, 212, 230) rgb(188, 212, 230) rgb(113, 165, 203); border-image: initial; transform: translateZ(0px); animation: spinnerRotate 0.8s linear 0s infinite normal none running; }

